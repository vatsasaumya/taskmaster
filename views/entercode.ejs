<%- include("partials/header") %>
<div class="container">
	<div class="row">
		<h1 style="text-align: center;">Enter the access code to join team.</h1>
		<div style="width: 30%; margin: 25px auto;">
			<form action="/join/<%= id %>" method="POST">
				<div class="form-group">
					<input class="form-control" type="text" name="access" placeholder="Accss Code">
				</div>
				<div class="form-group">
					<button class="btn btn-lg btn-default btn-block btn-primary">Submit!</button>
				</div>
			</form>
		</div>
	</div>
</div>